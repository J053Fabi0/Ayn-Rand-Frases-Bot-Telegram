{
  "tasks": {
    "stop": "pm2 stop aynrandbot",
    "start": "DENO_DEPLOYMENT_ID=\"$(git rev-parse HEAD)\" pm2 start --interpreter='deno' --interpreter-args='run --allow-sys --allow-net --allow-read --allow-env --allow-run --allow-write' --name aynrandbot main.ts && pm2 logs aynrandbot",
    "logs": "pm2 logs aynrandbot",
    "reload": "pm2 reload aynrandbot && pm2 logs aynrandbot",
    "delete": "pm2 delete aynrandbot",
    "check": "deno check main.ts dev.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "run": "deno run --allow-net --allow-sys --allow-read --allow-env --allow-run --allow-write main.ts",
    "watch": "deno run --watch --allow-sys --allow-net --allow-read --allow-env --allow-run --allow-write dev.ts",
    "dev": "deno run --watch --allow-sys --allow-net --allow-read --allow-env --allow-run --allow-write dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "run:molt": "deno run --allow-env --allow-read --allow-write=. --allow-run=git,deno --allow-net=deno.land,esm.sh,cdn.jsdelivr.net https://deno.land/x/molt/cli.ts",
    "update_deps": "deno task -q run:molt check ./**/*.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.0",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "$fresh/": "https://deno.land/x/fresh@1.5.4/",
    "bcrypt": "https://deno.land/x/bcrypt@v0.4.1/mod.ts",
    "croner": "https://deno.land/x/croner@7.0.5/dist/croner.js",
    "dayjs/": "https://deno.land/x/deno_dayjs@v0.5.0/",
    "duplicatedFilesCleaner": "https://deno.land/x/duplicated_files_cleaner_incognito@1.2/mod.ts",
    "escape_html": "https://deno.land/x/escape_html@1.0.0/mod.ts",
    "fresh-session/": "https://deno.land/x/fresh_session@0.2.4/",
    "grammy/": "https://deno.land/x/grammy@v1.19.2/",
    "lodash": "https://deno.land/x/deno_ts_lodash@0.0.1/mod.ts",
    "mongo": "https://deno.land/x/mongo@v0.32.0/mod.ts",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.2",
    "preact": "https://esm.sh/preact@10.19.2",
    "preact/": "https://esm.sh/preact@10.19.2/",
    "react-icons/ai": "https://cdn.jsdelivr.net/gh/urielch/react-icons-ai@1.0.9/mod.ts",
    "react-icons/bs": "https://cdn.jsdelivr.net/gh/urielch/react-icons-bs@1.0.9/mod.ts",
    "react-icons/fi": "https://cdn.jsdelivr.net/gh/urielch/react-icons-fi@1.0.9/mod.ts",
    "shuffle": "https://deno.land/x/shuffle@v1.0.1/mod.ts",
    "sleep": "https://deno.land/x/sleep@v1.2.1/mod.ts",
    "std/": "https://deno.land/std@0.208.0/",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/",
    "web_bson": "https://deno.land/x/web_bson@v0.3.0/mod.js"
  },
  "lint": {
    "rules": {
      "tags": ["fresh", "recommended"]
    }
  },
  "exclude": ["**/_fresh/*"]
}
